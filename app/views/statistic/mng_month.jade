block content
    .content-page
        .content
            .container
                .row
                    .col-sm-12
                        .page-title-box
                            ol.breadcrumb.pull-right
                                li
                                    a(href='#') Home
                                li
                                    a(href='#') SD 통계
                                li.active 담당자별 월별 처리내역
                            h4.page-title 담당자별 월별 처리내역
                .row
                    .col-lg-12
                        .card-box.m-t-10
                            form.form-horizontal.group-border-dashed(action='#')
                                .form-group
                                    label.col-sm-2.control-label 검색년도
                                    .col-sm-2
                                        select#yyyy.form-control
                                            option(value='2012') 2012
                                            option(value='2013') 2013
                                            option(value='2014') 2014
                                            option(value='2015') 2015
                                            option(value='2016') 2016
                                            option(value='2017') 2017
                                            option(selected='2018') 2018
                                            option(value='2019') 2019
                                            option(value='2020') 2020
                                            option(value='2021') 2021
                                    .col-sm-2
                                        select#mm.form-control
                                            option(value='*') 전체
                                            option(value='01') 1월
                                            option(value='02') 2월
                                            option(value='03') 3월
                                            option(value='04') 4월
                                            option(value='05') 5월
                                            option(value='06') 6월
                                            option(value='07') 7월
                                            option(value='08') 8월
                                            option(value='09') 9월
                                            option(value='10') 10월
                                            option(value='11') 11월
                                            option(value='12') 12월
                                .form-group
                                    label.col-sm-2.control-label 담당자
                                    .col-sm-2
                                        select#manager.form-control
                                    label.col-sm-2.control-label 상위업무
                                    .col-sm-2
                                        select#higher_cd.form-control
                                    .col-sm-2
                                        button.btn.btn-primary(type='button')
                                            i.fa.fa-search.m-r-5
                                            | 조회
                .row
                    .col-lg-12
                        .card-box
                            table#target-table.table.table-bordered
                                thead
                                    tr
                                        th.font-12(class='text-center' width='10%') 월
                                        th.font-12(class='text-center' width='15%') 상위업무
                                        th.font-12(class='text-center' width='20%') 하위업무
                                        th.font-12(class='text-center' width='10%') 접수건수
                                        th.font-12(class='text-center' width='10%') 해결건수
                                        th.font-12(class='text-center' width='10%') 해결률(%)
                                        th.font-12(class='text-center' width='10%' ) 평점
                                tbody#more_list
               
               
block footer